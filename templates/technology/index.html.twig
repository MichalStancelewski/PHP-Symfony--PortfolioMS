{% extends 'base.html.twig' %}

{% block title %}Technologies{% endblock %}

{% block body %}

    <h1>Technologies</h1>

    <h2>List of used technologies</h2>


    <div class="container">
        <div class="row">
            {% for technology in technologies %}
                <div class="col-lg-4 col-md-6">
                    <li><a class="navigation-element"
                           href="{{ path('technologies_single', { slug: technology.slug }) }}">{{ technology }}</a>
                    </li>
                </div>
            {% endfor %}
        </div>
    </div>

{% endblock %}

