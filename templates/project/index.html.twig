{% extends 'base.html.twig' %}

{% block title %}Projects{% endblock %}

{% block body %}

    <h1>Projects</h1>

    <h2>List of my projects</h2>
    {% for project in projects %}
        <p>{{ project }} <a href="{{ path('project', { slug: project.slug }) }}">View</a></p>
    {% endfor %}

    {% for project in projects %}
        <li><a class="navigation-element" href="{{ path('project', { slug: project.slug }) }}">{{ project }}</a>
        </li>
    {% endfor %}

    <h2>List of used technologies</h2>
    {% for technology in technologies %}
        <p>{{ technology }}</p>
    {% endfor %}

{% endblock %}
